@model IEnumerable<DeviceManager.Models.HistoryViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ItemId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RequirementUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RequirementDateStart)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RequirementDateEnd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RequirementNote)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FixedUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FixedDateStart)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FixedDateEnd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FixedNote)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Result)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ItemId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RequirementUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RequirementDateStart)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RequirementDateEnd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RequirementNote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FixedUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FixedDateStart)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FixedDateEnd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FixedNote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Result)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
